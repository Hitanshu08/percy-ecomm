    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@playwright/test": "^1.40.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^23.0.1",
    "msw": "^2.0.8",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.0.0",
